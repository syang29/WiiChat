<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content">
    <corners android:radius="1dp" />
    <stroke
        android:width="1dp"
        android:color="#920e6597" />
    <gradient
        android:startColor="#f4eeee"
        android:centerColor="#ece0e0"
        android:endColor="#f5f1f1"
        android:angle="270" />
    <size
        android:width="15dp"
        android:height="15dp" />
</shape>